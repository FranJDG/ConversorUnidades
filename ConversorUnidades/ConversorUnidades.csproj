<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>pngwing.com.ico</ApplicationIcon>
    <PackageIcon>pngwing.com.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="pngwing.com.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\pngwing.com.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
